    "target": "ES5",
    "lib": ["dom", "es2015"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true, 
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"]
